      "outDir": "./dist/",
      "noImplicitAny": true,
      "sourceMap": true,
      "module": "commonjs",
      "target": "es5",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strictNullChecks": true,
      "declaration": true
