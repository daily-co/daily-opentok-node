{
  "name": "daily-opentok-node",
  "version": "0.0.1",
  "description": "A library to enable seamless substitution of OpenTok with Daily's REST API",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/opentok.cjs",
  "module": "./dist/opentok.mjs",
  "exports": {
    ".": {
      "import": "./dist/opentok.mjs",
      "require": "./dist/opentok.cjs"
    }
  },
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "jest": "jest",
    "test": "npm run lint && jest",
    "lint": "eslint src/",
    "lintFix": "eslint src/ --fix"
  },
  "author": "Liza Shulyayeva",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/opentok": "^2.13.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "jsonwebtoken": "^8.5.1",
    "opentok": "^2.14.3"
  }
}
